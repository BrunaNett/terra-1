<h2>These are all your listings</h2>
<%= @seller.name %>
<% @seller_listings.each do |listing|%>
	<p> Title: <%= listing.title %></br></p>
	<%= listing.description %></br>
	<%= listing.acres %></br>
	<%= listing.price %></br>
  <!-- add a link to be able to edit each listing -->
<p><%= link_to "Edit This Listing", edit_listing_path(listing, :seller_id => @seller.id)%></br></p>
<% end %>

<p><%= link_to "Create Another Listing", new_seller_path, class: "new_seller"%></br></p>
<p><%= link_to "Homepage", root_path%></br><p/>
