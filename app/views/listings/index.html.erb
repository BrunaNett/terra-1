<h1>All Listings</h1>
<% @listings.each do |listing|%>
	<ul>
    <li><%= link_to listing.title,listing_path(listing) %> </li>
    <%= image_tag listing.image.url(:thumb) %>
	</ul>
<% end %>

<p> <%= link_to "Back to Homepage",root_path%> </p>
<p> <%= link_to "Create New Listing",new_seller_path%> </p>

